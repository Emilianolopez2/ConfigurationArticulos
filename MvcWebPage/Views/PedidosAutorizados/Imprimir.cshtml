@using MvcWebPage.MLAVID

<input type="button" value="Imprimir" id="btnImprimir" />

<script>

    $("#btnImprimir").jqxButton({ width: 70, height: 30, template: "success" });

    $("#btnImprimir").click(function (e) {

        $(".footer").css('display','none');
        window.print();
        $(".footer").css('display', '');
    });
    
    
    
</script>
